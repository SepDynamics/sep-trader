{
  "mcpServers": {
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "alwaysAllow": [
        "create_entities",
        "create_relations",
        "add_observations",
        "delete_entities",
        "delete_observations",
        "search_nodes",
        "read_graph",
        "delete_relations",
        "open_nodes"
      ]
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "alwaysAllow": [
        "resolve-library-id",
        "get-library-docs"
      ]
    },
    "robust_build_analyzer": {
      "command": "python3",
      "args": [
        "/sep/.kilocode/mcp/tools/robust_build_analyzer/main.py"
      ],
      "alwaysAllow": [
        "analyze_build"
      ],
      "disabled": true
    }
  }
}